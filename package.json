{
  "name": "f-server",
  "version": "1.0.0",
  "description": "基于 Egg.js 的前端部署系统",
  "dependencies": {
    "axios": "^0.18.0",
    "egg": "^2.2.1",
    "egg-cors": "^2.1.0",
    "egg-scripts": "^2.5.0",
    "egg-sequelize": "^3.1.5",
    "egg-validate-joi": "^1.1.2",
    "egg-validator": "^0.0.4",
    "mysql2": "^1.6.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-f-server",
    "stop": "egg-scripts stop --title=egg-server-f-server",
    "dev": "export DOMAIN=saber-test-wms.haimaiche.net && egg-bin dev",
    "migrate:new": "egg-sequelize migration:create --name init",
